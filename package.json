{
  "name": "desk",
  "version": "1.1.4",
  "description": "Light-weight IDE",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "pack": "build --dir",
    "build": "electron-packager . Desk",
    "package-linux": "electron-packager . Desk --overwrite --asar=true --platform=linux --arch=x64 --icon=icon.png --prune=true --out=release-builds"
  },
  "build": {
    "appId": "com.desk.tech",
    "productName": "Desk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svr8/Desk.git"
  },
  "keywords": [
    "light-weight",
    "editor",
    "ide"
  ],
  "author": "Shikhar Vaish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svr8/Desk/issues"
  },
  "homepage": "https://github.com/svr8/Desk#readme",
  "devDependencies": {
    "electron": "^2.0.3",
    "electron-builder": "^20.19.1"
  },
  "dependencies": {
    "electron-json-config": "^1.5.2"
  }
}
